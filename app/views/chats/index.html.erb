<div class="scroll-3">
  Previous Games with Users:
  <ul>
   <% if @existing_chats_users %>
     <% @existing_chats_users.each do |other_user| %>
      <% if other_user != current_user %>
        <li>
          <%= link_to(user_chats_path(current_user, :other_user => other_user.id), method: :post) do %>
          <%= other_user.username %>
          <% end %>
        </li>
       <% end %>
     <% end %>
    <% end %>
  </ul>
</div>
<div class="scroll-3">
  Current Players Online:
  <ul>
    <% User.all.each do |other_user| %>
     <% if other_user != current_user %>
       <li>
         <%= link_to(user_chats_path(current_user, :other_user => other_user.id), method: :post) do %>
         <%= other_user.username %>
         <% end %>
       </li>
      <% end %>
    <% end %>
  </ul>
</div>
<div class="center">
  <%= link_to "Home", games_path %>
</div>
